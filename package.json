{
  "name": "universal-boilerplate",
  "description": "universal-boilerplate",
  "keywords": [
    "universal"
  ],
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sahlouls/universal-boilerplate.git"
  },
  "author": {
    "name": "Sahbi Sahloul"
  },
  "scripts": {
    "build": "set NODE_ENV='production' && webpack -p -c --progress",
    "start": "set NODE_ENV='production' && pm2 start server/runner.js --name universal-boilerplate",
    "nodemon": "nodemon --exec babel-node -- server/runner.js",
    "webpack-devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8050 --host 0.0.0.0",
    "lint": "eslint shared client server",
    "stop": "set NODE_ENV='production' && pm2 delete universal-boilerplate",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "bugs": {
    "url": "https://github.com/sahlouls/universal-boilerplate/issues"
  },
  "homepage": "https://github.com/sahlouls/universal-boilerplate#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "classnames": "^2.2.3",
    "compression": "^1.6.0",
    "cookie": "^0.2.3",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "flux-constants": "^0.2.2",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.0",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "object-assign": "^4.0.1",
    "qs": "^6.0.1",
    "react": "^0.14.5",
    "react-addons-css-transition-group": "^0.14.5",
    "react-addons-pure-render-mixin": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-modal": "^0.6.1",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^0.14.5",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
